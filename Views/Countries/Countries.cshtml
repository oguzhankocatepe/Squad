@model IEnumerable<Squad.Models.Country>

@{
    ViewData["Title"] = "Countries";
}

<h1>Countries</h1>

<table class="table">
    <tbody>
            @{
            
                int count = 0;
                @foreach (var item in Model) {
                <tr>
                    <td>
                                @Html.ActionLink(item.Name, "Players", "Players", new {  id=item.Code  }) 
                    </td>
                </tr>

                }

        }
    </tbody>
</table>
